DATABASE_URL="postgresql://user:password@host/database?sslmode=require"

# GitHub OAuth - Create at https://github.com/settings/developers
# Callback URL: http://localhost:5173/login/github/callback
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Base URL for OAuth callbacks (optional for local dev)
ORIGIN=http://localhost:5173

# Cryptomus - Get from https://cryptomus.com/
CRYPTOMUS_MERCHANT_ID=
CRYPTOMUS_API_KEY=

# Cron secret for token reset (generate a random string)
CRON_SECRET=

# Vercel Blob - Add from Vercel dashboard (Storage > Blob)
BLOB_READ_WRITE_TOKEN=

# ComfyUI - Running instance with Cloudflare tunnel
COMFYUI_URL=https://your-tunnel.trycloudflare.com
COMFYUI_TOKEN=your-cloudflare-tunnel-token

# Preview access (for Vercel preview deployments)
# Set PREVIEW_ACCESS_TOKEN in Vercel preview environment
# Then access preview with ?preview_token=your-secret-token
PREVIEW_ACCESS_TOKEN=
