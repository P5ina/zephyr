DATABASE_URL="postgresql://user:password@host/database?sslmode=require"

# fal.ai - Get from https://fal.ai/dashboard/keys
FAL_KEY=

# GitHub OAuth - Create at https://github.com/settings/developers
# Callback URL: http://localhost:5173/login/github/callback
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Base URL for OAuth callbacks (optional for local dev)
ORIGIN=http://localhost:5173

# NowPayments - Get from https://nowpayments.io/
NOWPAYMENTS_API_KEY=
NOWPAYMENTS_IPN_SECRET=

# Cron secret for token reset (generate a random string)
CRON_SECRET=

# Vercel Blob - Add from Vercel dashboard (Storage > Blob)
BLOB_READ_WRITE_TOKEN=

# Vast.ai - Get from https://cloud.vast.ai/account/
VAST_API_KEY=
VAST_DOCKER_IMAGE=your-dockerhub-user/comfyui-worker:latest
VAST_IDLE_TIMEOUT=600
VAST_GPU_MIN_RAM=24
